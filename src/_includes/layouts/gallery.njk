---
layout: layouts/default.njk
eleventyExcludeFromCollections: true
---
<section>
	<h1>{{ title }}<a href="/{{ title }}.xml" title="RSS feed"><img src="/icons/rss-simple.svg" alt="RSS Feed"></a></h1>

{%- set photos = collections.photo | reverse -%}
<div class="h-feed grid">
	{% for post in photos -%}
	<a href="{{ post.url }}">
		{%- set photo = (post.data.photo)-%}
		{%- include "partials/photo.njk" -%}
	</a>
	{%- endfor %}
</div>
</section>